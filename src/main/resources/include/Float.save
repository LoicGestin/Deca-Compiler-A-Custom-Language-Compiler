class Float {

    float POSITIVE_INFINITY = Math.pow(2,255);
    float NEGATIVE_INFINITY =  -this.POSITIVE_INFINITY;

    float NaN = Math.pow(2,255)*(1+0.5);

    float MAX_VALUE = 0x1.fffffep127;
    float MIN_VALUE = 0x0.000002p-126 ;

    bool isInfinite(float f){
        return (f==Float.POSITIVE_INFINITY || f== Float.NEGATIVE_INFINITY);
    }

    bool isNaN(float f){
        return f==Float.NaN;
    }

    int sign(float f){
        if (f<0){
            return -1;
        }
        return 1;
    }

    float abs(float f){
        if (f>=0){
            return f;
        }
        else{
            return -f;
        }
    }

}